## CONEXION A LA BASE DE DATOS

DB_URL=jdbc:mysql://(IP:PORT)/(databaseName)?createDatabaseIfNotExist=true
MYSQL_USER=mysql_user
MYSQL_PASSWORD=mysql_password
HIBERNATE_DDL=create / create-drop / update (default: none)
LOG_SQL=true/false (default: false)
FORMAT_SQL=true/false (default: false)
SECRET_KEY=generated 256 bit hexadecimal key
TOKEN_EXPIRATION_TIME=token expiration time in minutes
JWT_ISSUER=issuer sign